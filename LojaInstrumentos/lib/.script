CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE CLIENTE(CPF INTEGER NOT NULL PRIMARY KEY,RG INTEGER NOT NULL,NOME VARCHAR(20) NOT NULL,SOBRENOME VARCHAR(50) NOT NULL,SEXO CHAR NOT NULL,DATANASCIMENTO DATE NOT NULL,DATACADASTRO DATE NOT NULL,ESTADOCIVIL CHAR NOT NULL,TELEFONE VARCHAR(13) NOT NULL,CELULAR VARCHAR(13) NOT NULL,EMAIL VARCHAR(50) NOT NULL,ENDERECO VARCHAR(50) NOT NULL,LOGADOURO VARCHAR(50) NOT NULL,BAIRRO VARCHAR(50) NOT NULL,NUMERO INTEGER NOT NULL,CEP INTEGER NOT NULL,ESTADO VARCHAR(2) NOT NULL,COMPLEMENTO VARCHAR(50) NOT NULL,REFERENCIA VARCHAR(100) NOT NULL)
CREATE MEMORY TABLE USUARIO(CPF INTEGER NOT NULL PRIMARY KEY,RG INTEGER NOT NULL,NOME VARCHAR(20) NOT NULL,SOBRENOME VARCHAR(50) NOT NULL,EMAIL VARCHAR(50) NOT NULL,USER VARCHAR(30) NOT NULL,SENHA VARCHAR(30) NOT NULL)
CREATE MEMORY TABLE PRODUTO(CODIGO INTEGER NOT NULL PRIMARY KEY,NOME VARCHAR(20) NOT NULL,MARCA VARCHAR(50) NOT NULL,MODELO VARCHAR(50) NOT NULL,DESCRICAO VARCHAR(50) NOT NULL,DATACOMPRA DATE NOT NULL,QUANTIDADE INTEGER NOT NULL,VALORUNITARIO DECIMAL NOT NULL,VALORCOMPRA DECIMAL NOT NULL)
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 10
SET SCHEMA PUBLIC
